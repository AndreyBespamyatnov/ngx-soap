    "lodash": "^4.17.4",
    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.0.1",
    "sax": "^1.2.4",
    "selectn": "^1.1.2",
    "strip-bom": "^3.0.0",
    "typescript": "^2.3.4",
    "uglify-js": "^3.0.17",
    "url": "^0.11.0",
